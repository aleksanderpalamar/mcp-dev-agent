{
    "models": {
        "codellama": {
            "provider": "ollama",
            "model": "codellama",
            "parameters": {
                "temperature": 0.7,
                "context_window": 4096,
                "top_p": 0.9
            }
        },
        "llama2": {
            "provider": "ollama",
            "model": "llama3",
            "parameters": {
                "temperature": 0.7,
                "context_window": 4096,
                "top_p": 0.9
            }
        },
        "mistral": {
            "provider": "ollama",
            "model": "mistral",
            "parameters": {
                "temperature": 0.7,
                "context_window": 4096,
                "top_p": 0.9
            }
        }
    },
    "default_model": "codellama",
    "security": {
        "use_env_variables": true,
        "required_env_vars": [
            "GITHUB_TOKEN"
        ]
    },
    "cache": {
        "enabled": true,
        "directory": ".cache",
        "max_age": 86400
    },
    "logging": {
        "enabled": true,
        "level": "INFO",
        "file": "agent.log"
    }
}