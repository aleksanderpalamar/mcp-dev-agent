{
    "models": {
        "claude": {
            "provider": "anthropic",
            "model": "claude-3-opus-20240229",
            "api_key": "${ANTHROPIC_API_KEY}",
            "parameters": {
                "temperature": 0.7,
                "max_tokens": 4096,
                "top_p": 0.9
            }
        },
        "gpt4": {
            "provider": "openai",
            "model": "gpt-4-turbo-preview",
            "api_key": "${OPENAI_API_KEY}",
            "parameters": {
                "temperature": 0.7,
                "max_tokens": 4096,
                "top_p": 0.9,
                "frequency_penalty": 0,
                "presence_penalty": 0
            }
        },
        "gpt35": {
            "provider": "openai",
            "model": "gpt-3.5-turbo",
            "api_key": "${OPENAI_API_KEY}",
            "parameters": {
                "temperature": 0.7,
                "max_tokens": 4096,
                "top_p": 0.9,
                "frequency_penalty": 0,
                "presence_penalty": 0
            }
        }
    },
    "default_model": "claude",
    "api_base": {
        "anthropic": "https://api.anthropic.com/v1",
        "openai": "https://api.openai.com/v1"
    },
    "security": {
        "use_env_variables": true,
        "required_env_vars": [
            "ANTHROPIC_API_KEY",
            "OPENAI_API_KEY",
            "GITHUB_TOKEN"
        ]
    },
    "cache": {
        "enabled": true,
        "directory": ".cache",
        "max_age": 86400
    },
    "logging": {
        "enabled": true,
        "level": "INFO",
        "file": "agent.log"
    }
}